<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="FileManagerData">
        <property name="file" type="File"/>
    </transfer>

    <transfer name="ContentFileListTerm">
        <property name="fileIds" type="int[]" singular="fileIds"/>
    </transfer>

    <transfer name="LocalizedContent">
        <property name="fkLocale" type="int"/>
    </transfer>

    <transfer name="Locale">
        <property name="idLocale" type="int"/>
    </transfer>

    <transfer name="ContentValidationResponse">
        <property name="parameterMessages" type="ContentParameterMessage[]" singular="parameterMessages"/>
        <property name="isSuccess" type="bool"/>
    </transfer>

    <transfer name="ContentParameterMessage">
        <property name="messages" type="Message[]" singular="message"/>
        <property name="parameter" type="string"/>
    </transfer>

    <transfer name="Message">
        <property name="parameters" type="array" singular="parameters"/>
        <property name="value" type="string"/>
    </transfer>

    <transfer name="ContentWidgetTemplate">
        <property name="name" type="string"/>
        <property name="identifier" type="string"/>
    </transfer>

    <transfer name="File">
        <property name="idFile" type="int"/>
    </transfer>

</transfers>
